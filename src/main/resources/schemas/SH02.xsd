                <xs:element ref="number" minOccurs="1" maxOccurs="1" />
                <xs:element ref="value" minOccurs="1" maxOccurs="1" />
            </xs:all>
        </xs:complexType>
    </xs:element>

    <xs:element name="reconvertedClass">
